#include<stdio.h>
int main(){
  int v1 = 5, v2 = 2;
  printf("====================\n");
  printf("%d + %d = %d\n", v1, v2, v1+v2);
  printf("%d - %d = %d\n", v1, v2, v1-v2 );
  printf("%d x %d = %d\n", v1, v2, v1*v2);
  printf("%d / %d = %d\n", v1, v2, v1/v2);
  printf("Resto de: %d / %d = %d\n", v1, v2, v1%v2);
  printf("====================\n");
  return 0;
}
